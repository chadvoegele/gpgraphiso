triangles/tuxedo-12-raw.csv: $(wildcard cpu-logs/triangles/tuxedo-12/*.log)
	$(BMK2)/ps/log2csv.py -x tuxedo-12 $^ -o $@

triangles/tuxedo-12-gal.csv: $(wildcard cpu-logs/triangles/tuxedo-12/*.log)
